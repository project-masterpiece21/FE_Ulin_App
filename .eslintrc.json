{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "arrow-body-style": 0,
        "consistent-return": 0,
        "vue/no-multiple-template-root": 0,
        "unknownAtRules": 0,
        "quotes": 0
    }
}